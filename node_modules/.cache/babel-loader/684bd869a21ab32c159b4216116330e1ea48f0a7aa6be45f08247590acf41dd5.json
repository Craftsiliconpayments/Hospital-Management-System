{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n/* eslint-disable */\nimport NavigationBar from \"@/components/Navigation\";\nimport router from \"@/router\";\nimport { biller, execute } from \"@/api\";\nexport default {\n  name: \"BillsReceipted\",\n  components: {\n    NavigationBar\n  },\n  data() {\n    return {\n      keyword: '',\n      stream: '',\n      logsName: '',\n      search: '',\n      monthNames: ['', 'January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'],\n      monthName: '',\n      month: '',\n      receiptNo: '',\n      streams: [],\n      receiptDetail: {\n        id: \"151\",\n        receiptNo: \"BLL2023041621063411\",\n        billNo: \"BLL0416090620\",\n        detailAmount: \"20\",\n        receiptAmount: \"20\",\n        billBalance: \"0\",\n        costCenterNo: \"\",\n        accountNo: \",\",\n        incomeTypeDescription: \"Market Fees,\",\n        feeID: \"274,\",\n        wardID: \"472761372\",\n        subCountyID: \"47276\",\n        currency: \"KES\",\n        source: \"CASH\",\n        transactionCode: \"BLL0416090620\",\n        paidBy: \"#\",\n        dateCreated: \"2023-04-16 09:06:34\",\n        dateModified: \"2023-04-16 09:06:34\",\n        createdBy: \"1\",\n        modifiedBy: \"#\",\n        isActive: \"1\",\n        statu: \"PAID\"\n      },\n      receiptDetails: [],\n      receiptInfos: []\n    };\n  },\n  computed: {\n    totalReceiptAmount() {\n      return this.formatPrice(this.receiptDetails.reduce((sum, billDetail) => sum + parseInt(billDetail.receiptAmount), 0));\n    }\n  },\n  mounted() {\n    this.logsName = sessionStorage.getItem('logsName');\n    const d = new Date();\n    this.month = d.getMonth();\n    this.monthName = this.getCurrentMonth() + \" \" + this.getCurrentYear();\n    this.getReceipts();\n    this.$watch('search', function (newValue) {\n      this.keyword = newValue;\n      this.getReceipts();\n    });\n    this.$watch('stream', function (newValue) {\n      this.keyword = newValue;\n      this.getReceipts();\n    });\n  },\n  methods: {\n    getHumanDate(dateData) {\n      //const res = dateData.split(\" \");\n      const dateObject = new Date(Date.parse(dateData));\n      //console.log(dateObject.toDateString())\n\n      // Create a date object from the dateData string\n      //const dateObject = new Date(Date.parse(dateData));\n\n      // Define an array of weekday names\n      const weekdays = [\"Sun\", \"Mon\", \"Tue\", \"Wed\", \"Thu\", \"Fri\", \"Sat\"];\n\n      // Get the weekday name from the date object\n      const weekday = weekdays[dateObject.getDay()];\n\n      // Get the month number from the date object (0-11)\n      const month = dateObject.getMonth();\n\n      // Define an array of month names\n      const months = [\"Jan\", \"Feb\", \"Mar\", \"Apr\", \"May\", \"Jun\", \"Jul\", \"Aug\", \"Sep\", \"Oct\", \"Nov\", \"Dec\"];\n\n      // Get the month name from the month number\n      const monthName = months[month];\n\n      // Get the date from the date object (1-31)\n      const date = dateObject.getDate();\n\n      // Get the year from the date object\n      const year = dateObject.getFullYear();\n\n      // Get the hours from the date object (0-23)\n      const hours = dateObject.getHours();\n\n      // Convert the hours to 12-hour format (0-11)\n      const hours12 = hours % 12;\n\n      // Add a leading zero if the hours are less than 10\n      const hoursString = hours12 < 10 ? \"0\" + hours12 : \"\" + hours12;\n\n      // Get the minutes from the date object (0-59)\n      const minutes = dateObject.getMinutes();\n\n      // Add a leading zero if the minutes are less than 10\n      const minutesString = minutes < 10 ? \"0\" + minutes : \"\" + minutes;\n\n      // Determine if the time is AM or PM\n      const ampm = hours < 12 ? \"AM\" : \"PM\";\n\n      // Concatenate the date and time components into a formatted string\n      const formattedString = weekday + \" \" + monthName + \" \" + date + \" \" + year + \" At \" + hoursString + \":\" + minutesString + \" \" + ampm;\n\n      // Display the formatted string\n      //console.log(formattedString);\n\n      return formattedString;\n    },\n    getCurrentMonth() {\n      const d = new Date();\n      const monthName = this.monthNames[d.getMonth()];\n      console.log(monthName);\n      return monthName;\n    },\n    getMonthName() {\n      const monthNames = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];\n      const d = new Date();\n      const monthName = monthNames[d.getMonth()];\n      console.log(monthName);\n      return monthName;\n    },\n    getCurrentYear() {\n      const date = new Date();\n      const currentYear = date.getFullYear();\n      console.log(currentYear); // Output: 2023\n      return currentYear;\n    },\n    formatPrice(value) {\n      var formatter = new Intl.NumberFormat('en-US', {\n        style: 'currency',\n        currency: 'KES',\n        minimumFractionDigits: 2\n      });\n      return formatter.format(value);\n    },\n    gotTo(route) {\n      router.push(route);\n    },\n    getReceipts() {\n      this.receiptDetails = [];\n      const data = new FormData();\n      data.append(\"function\", \"getReceipts\");\n      data.append(\"keyword\", this.keyword);\n      data.append(\"idNo\", sessionStorage.getItem('idNoUser'));\n      execute(data, biller).then(res => {\n        if (res.data.success) {\n          this.receiptDetails = res.data.data.receiptDetails;\n          this.streams = res.data.data.streams;\n        } else {\n          this.message = res.data.message;\n        }\n      }).catch(e => {\n        //alert(e.message);\n        this.message = e.message;\n      });\n    },\n    getReceipt(receiptNo) {\n      this.receiptNo = receiptNo;\n      const data = new FormData();\n      data.append(\"function\", \"getReceipt\");\n      data.append(\"receiptNo\", receiptNo);\n      execute(data, biller).then(res => {\n        this.streams.splice(0);\n        if (res.data.success) {\n          this.receiptInfos = res.data.data.receiptInfos;\n          this.receiptDetail = res.data.data.receiptDetails;\n        } else {\n          this.message = res.data.message;\n        }\n      }).catch(e => {\n        //alert(e.message);\n        this.message = e.message;\n      });\n    },\n    printBill() {\n      localStorage['params'] = JSON.stringify({\n        billNo: this.billDetail.billNo\n      });\n      const routeData = this.$router.resolve({\n        name: 'bill'\n      });\n      window.open(routeData.href, '_blank');\n    },\n    printReceipt() {\n      if (this.receiptDetail.receiptNo == null) {\n        alert(\"Try again later\");\n      } else {\n        //  this.$refs.closeReceived.click();\n        localStorage['params'] = JSON.stringify({\n          receiptNo: this.receiptDetail.receiptNo\n        });\n        const routeData = this.$router.resolve({\n          name: 'receipt'\n        });\n        window.open(routeData.href, '_blank');\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavigationBar","router","biller","execute","name","components","data","keyword","stream","logsName","search","monthNames","monthName","month","receiptNo","streams","receiptDetail","id","billNo","detailAmount","receiptAmount","billBalance","costCenterNo","accountNo","incomeTypeDescription","feeID","wardID","subCountyID","currency","source","transactionCode","paidBy","dateCreated","dateModified","createdBy","modifiedBy","isActive","statu","receiptDetails","receiptInfos","computed","totalReceiptAmount","formatPrice","reduce","sum","billDetail","parseInt","mounted","sessionStorage","getItem","d","Date","getMonth","getCurrentMonth","getCurrentYear","getReceipts","$watch","newValue","methods","getHumanDate","dateData","dateObject","parse","weekdays","weekday","getDay","months","date","getDate","year","getFullYear","hours","getHours","hours12","hoursString","minutes","getMinutes","minutesString","ampm","formattedString","console","log","getMonthName","currentYear","value","formatter","Intl","NumberFormat","style","minimumFractionDigits","format","gotTo","route","push","FormData","append","then","res","success","message","catch","e","getReceipt","splice","printBill","localStorage","JSON","stringify","routeData","$router","resolve","window","open","href","printReceipt","alert"],"sources":["/Applications/XAMPP/xamppfiles/htdocs/Meru/county-biller/src/components/systemusers/UserCollections.vue"],"sourcesContent":["<template>\n    <head>\n\n        <meta charset=\"utf-8\" />\n        <title>NTSA | {{ logsName }}'s Details</title>\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n        <meta content=\"Keep your finances and membership up to date\" name=\"description\" />\n        <meta content=\"Kelvin Njuguna Ndungu\" name=\"author\" />\n        <!-- App favicon -->\n        <link rel=\"shortcut icon\" href=\"assets/images/favicon.svg\">\n\n        <!-- DataTables -->\n        <link href=\"assets/libs/datatables.net-bs4/css/dataTables.bootstrap4.min.css\" rel=\"stylesheet\" type=\"text/css\" />\n        <link href=\"assets/libs/datatables.net-buttons-bs4/css/buttons.bootstrap4.min.css\" rel=\"stylesheet\"\n            type=\"text/css\" />\n\n        <!-- Bootstrap Css -->\n        <link href=\"assets/css/bootstrap.min.css\" id=\"bootstrap-style\" rel=\"stylesheet\" type=\"text/css\" />\n        <!-- Bootstrap select styling -->\n        <link rel=\"stylesheet\"\n            href=\"https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/css/bootstrap-select.min.css\">\n        <!-- Icons Css -->\n        <link href=\"assets/css/icons.min.css\" rel=\"stylesheet\" type=\"text/css\" />\n        <!-- App Css-->\n        <link href=\"assets/css/app.min.css\" id=\"app-style\" rel=\"stylesheet\" type=\"text/css\" />\n        <!-- custom Css-->\n        <link href=\"assets/css/custom.css\" id=\"app-style\" rel=\"stylesheet\" type=\"text/css\" />\n\n\n    </head>\n\n    <body data-sidebar=\"dark\">\n\n\n\n        <!-- Begin page -->\n        <div id=\"layout-wrapper\">\n\n            <NavigationBar />\n\n\n            <!-- ============================================================== -->\n            <!-- Start right Content here -->\n            <!-- ============================================================== -->\n            <div class=\"main-content\">\n\n                <div class=\"page-content\">\n                    <div class=\"container-fluid\">\n\n                        <!-- start page title -->\n                        <div class=\"row\">\n                            <div class=\"col-12\">\n                                <div class=\"page-title-box d-sm-flex align-items-center justify-content-between\">\n                                    <h4 class=\"mb-sm-0 font-size-18\">Revenue Collections By {{ logsName }}</h4>\n\n                                    <div class=\"page-title-right\">\n                                        <ol class=\"breadcrumb m-0\">\n\n\n                                            <li class=\"breadcrumb-item\"><a @click=\"gotTo('dashboard')\"\n                                                    href=\"javascript: void(0)\">Dashboards</a></li>\n                                            <li class=\"breadcrumb-item\"><a @click=\"gotTo('users')\"\n                                                    href=\"javascript: void(0)\">All users</a></li>\n                                            <li class=\"breadcrumb-item\"><a>{{ logsName }}</a></li>\n                                            <li class=\"breadcrumb-item active\">User logs</li>\n                                        </ol>\n                                    </div>\n\n                                </div>\n                            </div>\n                        </div>\n                        <!-- end page title -->\n\n                        <!-- user details -->\n                        <div class=\"row\">\n                            <div class=\"col-lg-12\">\n                                <div class=\"card\">\n                                    <div class=\"card-body\">\n                                        <div class=\"row\">\n                                            <div class=\"col-lg-4\">\n                                                <div class=\"d-flex\">\n                                                    <div class=\"flex-shrink-0 me-3\">\n                                                        <div class=\"avatar-md mr-15px \">\n                                                            <h1 class=\"avatar-title rounded-circle \">\n                                                                CO\n                                                            </h1>\n                                                        </div>\n                                                    </div>\n                                                    <div class=\"flex-grow-1 align-self-center\">\n                                                        <div class=\"text-muted\">\n                                                            <h5 class=\"mb-1\">{{ logsName }}</h5>\n                                                            <p class=\"mb-0\">Revenue Colector (ID 1234567)</p>\n                                                            <p class=\"text-muted mb-0 d-flex align-items-center\">\n                                                                <a href=\"tel:0704549859\"\n                                                                    class=\"d-flex align-items-center\"><i\n                                                                        class=\"mdi mdi-phone me-2 font-size-18\"></i> 0704\n                                                                    549 859</a> <span class=\"px-3 px-3\">|</span>\n                                                                <a class=\"d-flex align-items-center\"\n                                                                    href=\"mailto:email@email.com\"><i\n                                                                        class=\"mdi mdi-email-outline font-size-18 me-2\"></i>\n                                                                    email@email.com</a>\n                                                            </p>\n                                                        </div>\n                                                    </div>\n                                                </div>\n                                            </div>\n\n                                            <div class=\"col-lg-6 align-self-center\">\n                                                <div class=\"text-lg-center mt-4 mt-lg-0\">\n                                                    <div class=\"row\">\n                                                        <div class=\"col-3\">\n                                                            <div>\n                                                                <p class=\"text-muted text-truncate mb-2\">Sub County</p>\n                                                                <h6 class=\"mb-0\">Ndhiwa Sub county</h6>\n                                                            </div>\n                                                        </div>\n                                                        <div class=\"col-3\">\n                                                            <div>\n                                                                <p class=\"text-muted text-truncate mb-2\">Ward</p>\n                                                                <h6 class=\"mb-0\">Kino Ward</h6>\n                                                            </div>\n                                                        </div>\n                                                        <div class=\"col-3\">\n                                                            <div>\n                                                                <p class=\"text-muted text-truncate mb-2\">Last Seen</p>\n                                                                <h6 class=\"mb-0\">10 Jun 2023 At 12:54 AM</h6>\n\n                                                            </div>\n                                                        </div>\n\n                                                        <div class=\"col-3\">\n                                                            <div>\n                                                                <p class=\"text-muted text-truncate mb-2\">Current Zone</p>\n                                                                <h6 class=\"mb-0\">Modern Market</h6>\n\n                                                            </div>\n                                                        </div>\n\n\n                                                    </div>\n                                                </div>\n                                            </div>\n\n                                            <div class=\"col-lg-2 d-lg-block\">\n                                                <div class=\"clearfix mt-4 mt-lg-0\">\n                                                    <div class=\"dropdown float-end\">\n                                                        <div class=\"d-flex flex-column me-3 pe-3\">\n                                                            <span class=\"pr-3 fw-semibold mb-2\">Added By</span>\n                                                            <a href=\"use-logs.html\">Elvice Weke On 23 Jan 2023 10:45 AM</a>\n                                                        </div>\n                                                    </div>\n                                                </div>\n                                            </div>\n                                        </div>\n                                        <!-- end row -->\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                        <!-- end of user details -->\n\n                        <!-- tool bar -->\n                        <div class=\"row\">\n                            <div class=\"col-12\">\n                                <div class=\"card\">\n                                    <div class=\"card-body mini-nav bg-primary pt-2 pb-3 active\">\n                                        <nav class=\"navbar navbar-expand-md navbar navbar-dark bg-primary py-2 active\">\n                                            <button\n                                                class=\"navbar-toggler btn btn-sm px-3 font-size-16 header-item waves-effect h-auto text-primary\"\n                                                type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarNavAltMarkup\"\n                                                aria-controls=\"navbarNavAltMarkup\" aria-expanded=\"false\"\n                                                aria-label=\"Toggle navigation\">\n                                                <span class=\"mdi mdi-menu\"></span>\n                                            </button>\n                                            <div class=\"collapse navbar-collapse justify-content-between\"\n                                                id=\"navbarNavAltMarkup\">\n                                                <div class=\"navbar-nav\">\n                                                    <a class=\"nav-item nav-link \" @click=\"gotTo('user-logs')\"\n                                                        href=\"javascript: void(0)\">User's Logs</a>\n                                                    <a class=\"nav-item nav-link active\" @click=\"gotTo('user-collections')\"\n                                                        href=\"javascript: void(0)\">Revenue Collections<span\n                                                            class=\"sr-only\">(current)</span></a>\n                                                    <!-- <a class=\"nav-item nav-link\" href=\"user-daily.html\">Daily Collections</a>-->\n                                                </div>\n                                                <!--   <div class=\"navbar-nav\">\n                                                <a href=\"collector-new.html\" type=\"button\" class=\"btn btn-primary waves-effect waves-light text-white\">\n                                                    <i class=\"bx bx bxs-edit-alt font-size-16 align-middle me-2\"></i> Edit Account\n                                                </a>\n                                            </div>-->\n\n                                            </div>\n                                        </nav>\n                                    </div>\n                                </div>\n\n                            </div>\n                        </div>\n\n                        <!-- end of tool bar -->\n\n                        <div class=\"row\">\n                            <div class=\"col-12\">\n                                <div class=\"card\">\n                                    <div\n                                        class=\"card-header bg-white pt-0 pr-0 p-0 d-flex justify-content-between align-items-center w-100 border-bottom\">\n\n                                        <div class=\"btn-toolbar p-3 align-items-center d-none animated delete-tool-bar\"\n                                            role=\"toolbar\">\n\n                                            <button type=\"button\"\n                                                class=\"btn btn-primary waves-effect btn-label waves-light me-3\"><i\n                                                    class=\"bx  bx-trash label-icon\"></i> Delete Contact(s)</button>\n                                            <button type=\"button\"\n                                                class=\"btn btn-primary waves-effect btn-label waves-light me-3\"><i\n                                                    class=\"mdi mdi-plus label-icon\"></i> Compose Message</button>\n                                        </div>\n                                    </div>\n\n                                    <div class=\"card-header bg-white w-100 border-bottom d-flex\">\n                                        <div class=\"mr-15px\">\n                                            <select v-model=\"stream\" class=\"form-select form-control bg-white mr-15px\"\n                                                title=\"Select System User Type\" data-live-search=\"true\" data-style=\"\">\n                                                <option selected value=\"\">All Revenue Streams</option>\n                                                <option v-for=\"(item, index) in streams\" :value=\"item.stream\" :key=\"index\">\n                                                    {{ item.stream }}</option>\n                                            </select>\n                                        </div>\n\n                                        <div class=\"flex-grow-1\">\n                                            <div class=\"search-box mb-0 me-2 flex-grow-1\">\n                                                <div class=\"position-relative\">\n                                                    <input v-model=\"search\" type=\"text\"\n                                                        class=\"form-control bg-light border-light rounded\"\n                                                        placeholder=\"Search ...\">\n                                                    <i class=\"bx bx-search-alt search-icon\"></i>\n                                                </div>\n                                            </div>\n                                        </div>\n\n\n                                    </div>\n                                    <div class=\"card-body\">\n                                        <div class=\"table-responsive\">\n                                            <table class=\"table align-middle table-nowrap table-hover  contacts-table\"\n                                                id=\"\">\n                                                <thead class=\"table-dark\">\n                                                    <tr>\n                                                        <th scope=\"col\">Date</th>\n                                                        <th>Paid By</th>\n                                                        <th>Transaction Code</th>\n                                                        <th>Revenue Stream</th>\n                                                        <th>Payment Mode</th>\n                                                        <th>Payment Status</th>\n                                                        <th>Paid At</th>\n                                                        <th scope=\"col\" class=\"text-right\">Transaction Amount</th>\n\n                                                    </tr>\n                                                </thead>\n                                                <tbody>\n                                                    <tr v-for=\"(item, index) in receiptDetails\" :value=\"item\" :key=\"index\">\n                                                        <td scope=\"col\">{{ getHumanDate(item.dateCreated) }}</td>\n                                                        <td>\n                                                            <h5 class=\"font-size-14 mb-1 \"><a href=\"javascript: void(0); \"\n                                                                    class=\"text-dark text-capitalize\">{{ item.paidBy }}</a>\n                                                            </h5>\n                                                            <!--  <p class=\"text-muted mb-0 \">0704*****9</p>-->\n                                                        </td>\n                                                        <td>{{ item.transactionCode }}</td>\n                                                        <td>{{ item.incomeTypeDescription }}</td>\n                                                        <td>{{ item.source }}</td>\n                                                        <td>\n                                                            <span v-if=\"item.status == 'PAID'\"\n                                                                class=\"badge badge-soft-success text-uppercase\">{{ item.status }}</span>\n                                                            <span v-if=\"item.status == 'UNPAID'\"\n                                                                class=\"badge badge-soft-danger text-uppercase\">{{ item.status }}</span>\n                                                        </td>\n                                                        <td><small>{{ item.subCountyName }} {{ item.wardName }} {{ item.zone }}\n                                                            </small></td>\n                                                        <td scope=\"col\" class=\"text-right\">\n                                                            <strong class=\"fw-semibold text-black\">{{ item.currency }}\n                                                                {{ item.receiptAmount }} </strong>\n                                                        </td>\n                                                    </tr>\n\n                                                </tbody>\n                                            </table>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                        <!-- end row -->\n\n\n\n                    </div>\n                    <!-- container-fluid -->\n                </div>\n                <!-- End Page-content -->\n\n\n\n\n\n\n                <footer class=\"footer \">\n                    <div class=\"container-fluid \">\n                        <div class=\"row \">\n                            <div class=\"col-sm-6 \">\n                                <!--<script>\n                                document.write(new Date().getFullYear())\n                            </script> -->© Best Birdy\n                            </div>\n                            <div class=\"col-sm-6 \">\n                                <div class=\"text-sm-end d-sm-block \">\n                                    Best Birdy Billing system.\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </footer>\n            </div>\n            <!-- end main content-->\n\n        </div>\n        <!-- END layout-wrapper -->\n        <!-- Right bar overlay-->\n        <div class=\"rightbar-overlay\"></div>\n\n\n    </body>\n</template>\n\n<script>\n/* eslint-disable */\nimport NavigationBar from \"@/components/Navigation\";\nimport router from \"@/router\";\nimport { biller, execute } from \"@/api\";\nexport default {\n    name: \"BillsReceipted\",\n    components: { NavigationBar },\n    data() {\n        return {\n            keyword: '',\n            stream: '',\n            logsName: '',\n            search: '',\n            monthNames: ['', 'January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'],\n            monthName: '',\n            month: '',\n            receiptNo: '',\n            streams: [],\n            receiptDetail: {\n                id: \"151\",\n                receiptNo: \"BLL2023041621063411\",\n                billNo: \"BLL0416090620\",\n                detailAmount: \"20\",\n                receiptAmount: \"20\",\n                billBalance: \"0\",\n                costCenterNo: \"\",\n                accountNo: \",\",\n                incomeTypeDescription: \"Market Fees,\",\n                feeID: \"274,\",\n                wardID: \"472761372\",\n                subCountyID: \"47276\",\n                currency: \"KES\",\n                source: \"CASH\",\n                transactionCode: \"BLL0416090620\",\n                paidBy: \"#\",\n                dateCreated: \"2023-04-16 09:06:34\",\n                dateModified: \"2023-04-16 09:06:34\",\n                createdBy: \"1\",\n                modifiedBy: \"#\",\n                isActive: \"1\",\n                statu: \"PAID\"\n            },\n            receiptDetails: [],\n            receiptInfos: []\n        }\n    },\n    computed: {\n        totalReceiptAmount() {\n            return this.formatPrice(this.receiptDetails.reduce((sum, billDetail) => sum + parseInt(billDetail.receiptAmount), 0));\n        }\n    },\n    mounted() {\n        this.logsName = sessionStorage.getItem('logsName')\n        const d = new Date();\n        this.month = d.getMonth();\n        this.monthName = this.getCurrentMonth() + \" \" + this.getCurrentYear()\n\n        this.getReceipts()\n        this.$watch('search', function (newValue) {\n            this.keyword = newValue\n            this.getReceipts()\n        })\n        this.$watch('stream', function (newValue) {\n            this.keyword = newValue\n            this.getReceipts()\n        })\n    },\n    methods: {\n        getHumanDate(dateData) {\n            //const res = dateData.split(\" \");\n            const dateObject = new Date(Date.parse(dateData));\n            //console.log(dateObject.toDateString())\n\n            // Create a date object from the dateData string\n            //const dateObject = new Date(Date.parse(dateData));\n\n            // Define an array of weekday names\n            const weekdays = [\"Sun\", \"Mon\", \"Tue\", \"Wed\", \"Thu\", \"Fri\", \"Sat\"];\n\n            // Get the weekday name from the date object\n            const weekday = weekdays[dateObject.getDay()];\n\n            // Get the month number from the date object (0-11)\n            const month = dateObject.getMonth();\n\n            // Define an array of month names\n            const months = [\"Jan\", \"Feb\", \"Mar\", \"Apr\", \"May\", \"Jun\", \"Jul\", \"Aug\", \"Sep\", \"Oct\", \"Nov\", \"Dec\"];\n\n            // Get the month name from the month number\n            const monthName = months[month];\n\n            // Get the date from the date object (1-31)\n            const date = dateObject.getDate();\n\n            // Get the year from the date object\n            const year = dateObject.getFullYear();\n\n            // Get the hours from the date object (0-23)\n            const hours = dateObject.getHours();\n\n            // Convert the hours to 12-hour format (0-11)\n            const hours12 = hours % 12;\n\n            // Add a leading zero if the hours are less than 10\n            const hoursString = hours12 < 10 ? \"0\" + hours12 : \"\" + hours12;\n\n            // Get the minutes from the date object (0-59)\n            const minutes = dateObject.getMinutes();\n\n            // Add a leading zero if the minutes are less than 10\n            const minutesString = minutes < 10 ? \"0\" + minutes : \"\" + minutes;\n\n            // Determine if the time is AM or PM\n            const ampm = hours < 12 ? \"AM\" : \"PM\";\n\n            // Concatenate the date and time components into a formatted string\n            const formattedString = weekday + \" \" + monthName + \" \" + date + \" \" + year + \" At \" + hoursString + \":\" + minutesString + \" \" + ampm;\n\n            // Display the formatted string\n            //console.log(formattedString);\n\n            return formattedString;\n\n        