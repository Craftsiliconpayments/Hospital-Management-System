{"ast":null,"code":"/* eslint-disable */\nimport NavigationBar from \"@/components/Navigation\";\nimport ToolBarTargets from \"@/components/systemsettings/ToolBarTargets\";\nimport ToolBarStreams from \"@/components/reports/streams/ToolBar\";\nimport moment from \"moment\";\nimport { biller, execute, executeDownload } from \"@/api\";\nexport default {\n  name: \"ReportsStreamsSubcounties\",\n  components: {\n    ToolBarStreams,\n    ToolBarTargets,\n    NavigationBar\n  },\n  data() {\n    return {\n      download: 'Download Report',\n      documentType: '',\n      totalTransactions: 0,\n      totalAmount: 0,\n      totalAgents: 0,\n      dateTo: '',\n      dateFrom: '',\n      message: '',\n      alert: {\n        success: false,\n        info: false,\n        failed: false\n      },\n      contact: {\n        email: ''\n      },\n      password: '',\n      streams: [{\n        stream: {\n          incomeTypePrefix: \"ADVT\",\n          stream: \"ADVERTS\",\n          transactions: \"36\",\n          amount: \"848105\",\n          agents: \"5\"\n        },\n        subCountiesStreams: [{\n          subCountyName: \"BUURI EAST\",\n          amount: \"6200\"\n        }]\n      }]\n    };\n  },\n  mounted() {\n    this.dateFrom = moment().startOf('month').format('YYYY-MM-DD');\n    this.dateTo = moment().format('YYYY-MM-DD');\n    this.$watch('dateFrom', function (newValue) {\n      if (this.dateFrom === '' || this.dateTo === '') {} else {\n        this.getReportsStreamsSubCounty();\n      }\n    });\n    this.$watch('dateTo', function (newValue) {\n      if (this.dateFrom === '' || this.dateTo === '') {} else {\n        this.getReportsStreamsSubCounty();\n      }\n    });\n    this.getReportsStreamsSubCounty();\n  },\n  methods: {\n    numberFormat(num) {\n      const britishNumberFormatter = new Intl.NumberFormat(\"en-GB\");\n      return britishNumberFormatter.format(num);\n    },\n    downloadReportsStreamsSubCounty() {\n      this.download = \"Please wait..\";\n      const data = new FormData();\n      data.append(\"function\", \"downloadReportsStreamsSubCounty\");\n      data.append(\"dateFrom\", this.dateFrom);\n      data.append(\"dateTo\", this.dateTo);\n      executeDownload(data, biller).then(response => {\n        this.download = \"Download Report\";\n        const link = document.createElement('a');\n        link.href = URL.createObjectURL(response.data);\n        link.download = this.dateFrom + ' to ' + this.dateTo + \" ReportsStreams.csv\";\n        document.body.appendChild(link);\n        link.click();\n        document.body.removeChild(link);\n      });\n    },\n    getReportsStreamsSubCounty() {\n      this.streams.splice(0);\n      const data = new FormData();\n      data.append(\"function\", \"getReportsStreamsSubCounty\");\n      data.append(\"dateFrom\", this.dateFrom);\n      data.append(\"dateTo\", this.dateTo);\n      execute(data, biller).then(res => {\n        this.alert.info = false;\n        if (res.data.success) {\n          this.streams = res.data.data.streams;\n\n          /*  const totals = this.streams.reduce(\n                (accumulator, currentStream) => {\n                    // Convert string values to numbers for addition\n                    const transactions = parseInt(currentStream.transactions);\n                    const amount = parseInt(currentStream.amount);\n                    const agents = parseInt(currentStream.agents);\n                     // Add to the accumulator\n                    accumulator.totalTransactions += transactions;\n                    accumulator.totalAmount += amount;\n                    accumulator.totalAgents += agents;\n                     return accumulator;\n                },\n                {\n                    totalTransactions: 0,\n                    totalAmount: 0,\n                    totalAgents: 0,\n                }\n            );\n             this.totalTransactions = totals.totalTransactions\n            this.totalAmount = totals.totalAmount;\n            this.totalAgents = totals.totalAgents;*/\n        } else {\n          this.message = res.data.message;\n        }\n      }).catch(e => {\n        //alert(e.message);\n        this.message = e.message;\n      });\n    }\n  }\n};","map":{"version":3,"names":["NavigationBar","ToolBarTargets","ToolBarStreams","moment","biller","execute","executeDownload","name","components","data","download","documentType","totalTransactions","totalAmount","totalAgents","dateTo","dateFrom","message","alert","success","info","failed","contact","email","password","streams","stream","incomeTypePrefix","transactions","amount","agents","subCountiesStreams","subCountyName","mounted","startOf","format","$watch","newValue","getReportsStreamsSubCounty","methods","numberFormat","num","britishNumberFormatter","Intl","NumberFormat","downloadReportsStreamsSubCounty","FormData","append","then","response","link","document","createElement","href","URL","createObjectURL","body","appendChild","click","removeChild","splice","res","catch","e"],"sources":["/Applications/XAMPP/xamppfiles/htdocs/biller/meru-biller/src/components/reports/streams/ReportsStreamsSubcounties.vue"],"sourcesContent":["<template>\n\n<head>\n\n    <meta charset=\"utf-8\" />\n    <title>Hotline Transporters Sacco | All invoices</title>\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <meta content=\"Rental management system by Nouveta LTD\" name=\"description\" />\n    <meta content=\"Kelvin Njuguna\" name=\"author\" />\n    <!-- App favicon -->\n    <link rel=\"shortcut icon\" href=\"assets/images/favicon.svg\">\n    <!-- droping file -->\n    <link href=\"assets/libs/dropzone/min/dropzone.min.css\" rel=\"stylesheet\" type=\"text/css\" />\n\n    <!-- DataTables -->\n    <link href=\"assets/libs/datatables.net-bs4/css/dataTables.bootstrap4.min.css\" rel=\"stylesheet\" type=\"text/css\" />\n    <link href=\"assets/libs/datatables.net-buttons-bs4/css/buttons.bootstrap4.min.css