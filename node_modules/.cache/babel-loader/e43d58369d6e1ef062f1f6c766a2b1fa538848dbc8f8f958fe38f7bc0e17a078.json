{"ast":null,"code":"/* eslint-disable */\nimport { authUrl, execute } from \"@/api\";\nimport router from \"@/router\";\nimport NavigationBar from \"@/components/Navigation\";\nexport default {\n  name: \"SetAccounts\",\n  components: {\n    NavigationBar\n  },\n  data() {\n    return {\n      message: null,\n      categories: [{\n        category: \"EXECUTIVE\",\n        count: \"3\"\n      }],\n      rights: [{\n        type: {\n          type: \"ACTIONS\"\n        },\n        roles: [{\n          id: \"60\",\n          type: \"ACTIONS\",\n          role: \"APPROVE SBP VALIDATION\"\n        }, {\n          id: \"61\",\n          type: \"ACTIONS\",\n          role: \"APPROVE SBP INSPECTION\"\n        }, {\n          id: \"62\",\n          type: \"ACTIONS\",\n          role: \"APPROVE SPB\"\n        }]\n      }]\n    };\n  },\n  computed: {\n    filteredDashboardRoles() {\n      // Use flatMap to get an array of roles with type \"DASHBOARD\"\n      return this.rights.flatMap(right => right.type.type === \"DASHBOARD\" ? right.roles : []);\n    },\n    filteredModulesRoles() {\n      // Use flatMap to get an array of roles with type \"DASHBOARD\"\n      return this.rights.flatMap(right => right.type.type === \"MODULES\" ? right.roles : []);\n    },\n    filteredMobileRoles() {\n      // Use flatMap to get an array of roles with type \"DASHBOARD\"\n      return this.rights.flatMap(right => right.type.type === \"MOBILE\" ? right.roles : []);\n    },\n    filteredSupervisoryRoles() {\n      // Use flatMap to get an array of roles with type \"DASHBOARD\"\n      return this.rights.flatMap(right => right.type.type === \"SUPERVISORY PLATFORM\" ? right.roles : []);\n    }\n  },\n  mounted() {\n    this.getRoles();\n    this.getCategories();\n  },\n  methods: {\n    getRoles() {\n      this.categories.splice(0);\n      const data = new FormData();\n      data.append(\"function\", \"getRoles\");\n      execute(data);\n      execute(data, authUrl).then(res => {\n        if (res.data.success) {\n          this.rights = res.data.data.rights;\n        } else {\n          this.message = res.data.message;\n        }\n      }).catch(e => {\n        //alert(e.message);\n        this.message = e.message;\n      });\n    },\n    getCategories() {\n      this.categories.splice(0);\n      const data = new FormData();\n      data.append(\"function\", \"getCategories\");\n      execute(data);\n      execute(data, authUrl).then(res => {\n        if (res.data.success) {\n          this.categories = res.data.data.categories;\n        } else {\n          this.message = res.data.message;\n        }\n      }).catch(e => {\n        //alert(e.message);\n        this.message = e.message;\n      });\n    }\n  }\n};","map":{"version":3,"names":["authUrl","execute","router","NavigationBar","name","components","data","message","categories","category","count","rights","type","roles","id","role","computed","filteredDashboardRoles","flatMap","right","filteredModulesRoles","filteredMobileRoles","filteredSupervisoryRoles","mounted","getRoles","getCategories","methods","splice","FormData","append","then","res","success","catch","e"],"sources":["/Applications/XAMPP/xamppfiles/htdocs/biller/meru-biller/src/components/systemusers/SetAccounts.vue"],"sourcesContent":["<template>\n\n<head>\n\n    <meta charset=\"utf-8\" />\n    <title>NTSA | Users </title>\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <meta content=\"Keep your finances and membership up to date\" name=\"description\" />\n    <meta content=\"Kelvin Njuguna Ndungu\" name=\"author\" />\n    <!-- App favicon -->\n    <link rel=\"shortcut icon\" href=\"assets/images/favicon.svg\">\n\n    <!-- DataTables -->\n    <link href=\"assets/libs/datatables.net-bs4/css/dataTables.bootstrap4.min.css\" rel=\"stylesheet\" type=\"text/css\" />\n    <link href=\"assets/libs/datatables.net-buttons-bs4/css/buttons.bootstrap4.min.css\" rel=\"stylesheet\" type=\"text/css\" />\n\n    <!-- Bootstr