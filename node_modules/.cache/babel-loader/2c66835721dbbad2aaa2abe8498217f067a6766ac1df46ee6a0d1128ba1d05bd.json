                                        <option selected=\"\" value=\"filter-all\">All Sub counties</option>\n                                                            <option value=\"filter-all\">Ndhiwa</option>\n                                                            <option value=\"filter-sub-county\">Rachuonyo</option>\n                                                            <option value=\"filter-wards\">Homa Bay town</option>\n\n                                                        </select>\n                                                    </div>\n                                            </div>\n                                        </div>\n\n                                        <div class=\"col-2\">\n                                            <div class=\"d-flex flex-column\"><small class=\"text-uppercase fw-semibold mb-3\">Ward</small>\n                                                <div class=\"d-flex align-items-center\">\n                                                        <select class=\"form-control form-select\" tittle=\"Filter Transactions By\">\n                                                            <option selected=\"\" value=\"filter-all\">All wards</option>\n                                                            <option value=\"filter-all\">Ndhiwa</option>\n                                                            <option value=\"filter-sub-county\">Rachuonyo</option>\n                                                            <option value=\"filter-wards\">Homa Bay town</option>\n\n                                                        </select>\n                                                    </div>\n                                            </div>\n                                        </div>\n\n                                        <div class=\"col-2\">\n                                            <div class=\"d-flex flex-column\"><small class=\"text-uppercase fw-semibold mb-3\">Other Licences</small>\n                                                <div class=\"d-flex align-items-center\">\n                                                        <select class=\"form-control form-select\" tittle=\"Filter Transactions By\">\n                                                            <option selected=\"\" value=\"filter-all\">All Businesses</option>\n                                                            <option value=\"filter-all\">Fire Licenses</option>\n                                                            <option value=\"filter-sub-county\">Liquor</option>\n\n                                                        </select>\n                                                    </div>\n                                            </div>\n                                        </div>\n\n                                        <!-- <div class=\"col-3 d-flex justify-content-end align-items-end\">\n                                            <div class=\"text-sm-end mt-4 mt-sm-0 justify-end d-flex text-right flex-column\">\n                                                <p class=\"text-muted mb-2\">Collected Data</p>\n                                                <h5>45 Businesses </h5>\n                                            </div>\n                                        </div> -->\n                                    </div>\n                                </div>\n\n\n                                <div class=\"card-body\">\n                                    <div class=\"table-responsive \">\n\n                                        <table class=\"table align-middle table-hover  contacts-table table-striped \" id=\"\">\n                                            <thead class=\"table-light\">\n                                                <tr class=\"table-dark\">\n                                                    <th scope=\"col\">NO.</th>\n                                                    <th scope=\"col\">Name</th>\n   