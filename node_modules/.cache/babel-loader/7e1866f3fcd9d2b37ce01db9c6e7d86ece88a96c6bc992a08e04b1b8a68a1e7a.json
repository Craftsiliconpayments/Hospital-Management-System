model=\"department\" type=\"text\" class=\"form-control\" placeholder=\"Enter department\">\n                                                </div>\n                                            </div>\n\n\n                                        </div>\n\n                                    <div v-if=\"message!==null\" class=\"alert alert-danger\" role=\"alert\">\n                                        {{message}}\n                                    </div>\n\n\n                                        <div class=\"row\">\n                                            <div class=\"col-lg-10\">\n                                                <button v-if=\"getRight('CREATE DEPARTMENT')\"  @click=\"addDepartment()\" type=\"submit\" class=\"btn btn-primary\"><i class=\"mdi mdi-account-plus-outline me-1\"></i>Save</button>\n                                            </div>\n                                        </div>\n\n                                </div>\n                            </div>\n                        </div>\n\n                        <div class=\"col-6\">\n                            <div class=\"card p-4\">\n\n                                <div>\n                                    <div class=\"form-group\">\n                                        <label for=\"\">All Departments({{departments.length}})<strong class=\"text-danger\">*</strong></label>\n\n                                        <table class=\"table table-striped table-sm font-12 sources-table\">\n                                            <thead>\n                                            <tr class=\"font-weight-bold text-black\">\n                                                <th scope=\"col\">No</th>\n                                                <th scope=\"col\">Department</th>\n\n                                            </tr>\n                                            </thead>\n                                            <tbody>\n                                    