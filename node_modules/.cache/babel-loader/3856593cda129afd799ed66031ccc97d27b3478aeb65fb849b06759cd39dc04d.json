{"ast":null,"code":"import { execute, biller } from \"@/api\";\nexport default {\n  name: \"printReceipt\",\n  data() {\n    return {\n      date: null,\n      receiptDetails: {\n        id: null,\n        receiptNo: null,\n        billNo: null,\n        detailAmount: null,\n        receiptAmount: null,\n        billBalance: null,\n        costCenterNo: null,\n        accountNo: null,\n        incomeTypeDescription: null,\n        feeID: null,\n        wardID: null,\n        subCountyID: null,\n        currency: null,\n        source: null,\n        transactionCode: null,\n        paidBy: null,\n        dateCreated: null,\n        dateModified: null,\n        createdBy: null,\n        modifiedBy: null,\n        isActive: null,\n        status: null\n      },\n      link: '',\n      receiptInfos: [],\n      county: {\n        id: null,\n        countyID: null,\n        countyName: null,\n        tagline: null,\n        smsusername: null,\n        smspassword: null,\n        contact: null,\n        center: null,\n        address: null,\n        email: null,\n        headline: null,\n        tagline2: null,\n        ussd: null,\n        logo: null,\n        bank: null,\n        powered: null,\n        stateLogo: null,\n        seal: null,\n        signature: null,\n        lat: null,\n        lng: null,\n        link: null,\n        paysol: null,\n        poweredByLogo: null,\n        mobileApp: null,\n        selfService: null,\n        QRlink: null\n      }\n    };\n  },\n  mounted() {\n    let params = {};\n    try {\n      params = JSON.parse(localStorage['params']);\n      console.log('####' + params.receiptNo);\n      this.getReceipt(params.receiptNo);\n    } catch (error) {\n      // ignore\n    }\n  },\n  methods: {\n    showDate() {\n      const dateObj = new Date();\n      const currentDate = dateObj.getDate() + \"/\" + (dateObj.getMonth() + 1) + \"/\" + dateObj.getFullYear();\n      return currentDate;\n    },\n    formatPrice(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    getReceipt(receiptNo) {\n      const data = new FormData();\n      data.append(\"function\", \"getReceipt\");\n      data.append(\"receiptNo\", receiptNo);\n      execute(data, biller).then(res => {\n        if (res.data.success) {\n          this.receiptDetails = res.data.data.receiptDetails;\n          this.receiptInfos = res.data.data.receiptInfos;\n          this.county = res.data.data.county;\n          // this.link = this.county.QRlink+''+this.receiptDetails.receiptNo;\n          this.link = \"https://api.qrserver.com/v1/create-qr-code/?size=300x300&data=\" + '' + this.receiptDetails.receiptNo;\n        } else {\n          alert(res.data.data.message);\n        }\n      }).catch(e => {\n        alert(e.message);\n      });\n    }\n  }\n};","map":{"version":3,"names":["execute","biller","name","data","date","receiptDetails","id","receiptNo","billNo","detailAmount","receiptAmount","billBalance","costCenterNo","accountNo","incomeTypeDescription","feeID","wardID","subCountyID","currency","source","transactionCode","paidBy","dateCreated","dateModified","createdBy","modifiedBy","isActive","status","link","receiptInfos","county","countyID","countyName","tagline","smsusername","smspassword","contact","center","address","email","headline","tagline2","ussd","logo","bank","powered","stateLogo","seal","signature","lat","lng","paysol","poweredByLogo","mobileApp","selfService","QRlink","mounted","params","JSON","parse","localStorage","console","log","getReceipt","error","methods","showDate","dateObj","Date","currentDate","getDate","getMonth","getFullYear","formatPrice","value","val","toFixed","replace","toString","FormData","append","then","res","success","alert","message","catch","e"],"sources":["/Applications/XAMPP/xamppfiles/htdocs/biller/meru-biller/public/PrintDocs/Receipts/PrintReceipt.vue"],"sourcesContent":["<template>\n  <head>\n    <meta http-equiv=\"Content-Type\" content=\"text/html; charset=UTF-8\"/>\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\"/>\n    <title>Receipt</title>\n    <link rel=\"shortcut icon\" href=\"images/favicon.png\" type=\"image/x-icon\"/>\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\"/>\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin/>\n    <link\n        href=\"https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap\"\n        rel=\"stylesheet\"\n    />\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\"/>\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin/>\n    <link\n        href=\"https://fonts.googleapis.com/css2?family=Yellowtail&display=swap\"\n        rel=\"stylesheet\"\n    />\n  </head>\n\n  <body\n      style=\"\n        padding: 0px;\n        margin: 0px;\n        font-family: 'Montserrat', sans-serif;\n        background-color: #a2a2a21c;\n        box-sizing: border-box;\">\n\n\n  <button class=\"print-btn\" onclick=\"window.print()\">\n    <img :src=\"'./assets/printer.svg'\" alt=\"Printer Icon\"/>\n  </button>\n\n  <button class=\"print-btn email-doc\" title=\"send On Email\"><img :src=\"'./assets/email-icon.png'\" alt=\"Printer Icon \">\n  </button>\n\n  <page\n      class=\"page\"\n      style=\"\n          width: 210mm;\n          height: 297mm;\n          display: flex;\n          position: relative;\n          flex-flow: column;\n          background-color: white;\n        \"\n  >\n\n\n    <!-- main content -->\n    <div style=\"padding: 8.5mm\">\n      <header\n          style=\"\n              display: flex;\n              height: 20mm;\n              align-items: center;\n              position: relative;\n              width: 100%;\n            \"\n      >\n        <div>\n          <img\n              :src=\"county.logo\"\n              style=\"height: 20mm; margin-right: 5mm\"\n          />\n        </div>\n        <div>\n          <h2\n              style=\"\n                  margin-top: 0px;\n                  font-size: 18px;\n                  text-transform: uppercase;\n                  color: #146fc5;\n                  font-weight: 700;\n                  margin-bottom: 1.2mm;\n                  padding-bottom: 0px;\n                \"\n          >\n            COUNTY GOVERNMENT OF {{county.countyName}}\n          </h2>\n\n          <h4\n              style=\"\n                  padding: 0px;\n                  margin: 0px;\n                  font-size: 18px;\n                  font-weight: 400;\n                \"\n          >\n            {{county.tagline}}\n          </h4>\n        </div>\n        <div\n            style=\"\n                position: absolute;\n                right: 0mm;\n                font-size: 10px;\n                text-transform: uppercase;\n                bottom: 4mm;\n              \"\n        >\n          <p style=\"padding: 0px; margin: 0px; text-align: right\">\n            Printed on {{showDate() }}\n          </p>\n        </div>\n      </header>\n      <div>\n        <img :src=\"'./assets/blue-line.png'\" style=\"width: 100%\"/>\n      </div>\n\n      <!-- contact information and QR code -->\n      <div style=\"display: flex; justify-content: space-between\">\n        <!-- contact information -->\n        <div style=\"font-size: 12px\">\n          <h2\n              style=\"\n                  margin-top: 0px;\n                  font-size: 18px;\n                  text-transform: uppercase;\n                  color: #146fc5;\n                  font-weight: 700;\n                  margin-bottom: 3.3mm;\n                  margin-top: 3.3mm;\n                  padding-bottom: 0px;\n                \"\n          >\n            Receipt For {{ receiptDetails.incomeTypeDescription }}\n          </h2>\n\n          <p style=\"padding: 0px; margin: 0px; padding-bottom: 1.5mm\">\n            Support Tel No: {{county.contact}}\n          </p>\n          <p style=\"padding: 0px; margin: 0px; padding-bottom: 1.5mm\">\n            Email:{{county.email}}\n          </p>\n          <p style=\"padding: 0px; margin: 0px; padding-bottom: 1.5mm\">\n            Office Line (Business Hours) {{county.contact}}\n          </p>\n\n          <p\n              style=\"\n                  padding: 0px;\n                  margin: 0px;\n                  padding-top: 4.3mm;\n                  padding-bottom: 1.5mm;\n                \"\n          >\n            Payment Received From:\n            <strong>{{ receiptDetails.paidBy }}</strong>\n          </p>\n          <p style=\"padding: 0px; margin: 0px; padding-bottom: 1.5mm\">\n            Receipt No. <strong>{{ receiptDetails.receiptNo }}</strong>\n          </p>\n        </div>\n\n        <!-- the qr code -->\n        <div\n            style=\"\n                height: 25.3mm;\n                margin-top: 3.3mm;\n                width: 25.3mm;\n                border: 2px solid #146fc5;\n                display: flex;\n                justify-content: center;\n                align-items: center;\n              \"\n        >\n          <img :src=\"link\" style=\"height: 25mm\"/>\n        </div>\n      </div>\n\n      <!-- the table -->\n\n      <!-- the table contents -->\n      <table\n          style=\"\n              margin-top: 9.3mm;\n              border-spacing: initial;\n              display: block;\n              min-height: 70mm;\n            \"\n      >\n        <thead\n            class=\"\"\n            style=\"\n                text-transform: uppercase;\n                font-weight: 700;\n                font-size: 12px;\n                background: #e2f2fd;\n                padding: 2.5mm;\n              \"\n        >\n        <tr>\n          <td style=\"padding: 2.5mm\">Fee description</td>\n          <td style=\"padding: 2.5mm\">For</td>\n          <td style=\"text-align: right; padding: 2.5mm; width: 50mm\">\n            Amount Paid\n          </td>\n        </tr>\n        </thead>\n        <tbody v-for=\"(item, index) in receiptInfos\" :key=\"`item-${index}`\" style=\"font-size: 12px\">\n        <tr>\n          <td style=\"padding: 2.5mm\">\n            {{ index + 1 }}. {{ receiptDetails.incomeTypeDescription }}\n          </td>\n          <td style=\"padding: 2.5mm\">\n            {{ item.feeDescription}} <strong>{{item.customer}}{{item.description}}</strong>\n          </td>\n          <td style=\"text-align: right; padding: 2.5mm\">\n            <strong>KES {{item.receiptAmount}}</strong>\n          </td>\n        </tr>\n        </tbody>\n      </table>\n\n      <div>\n        <img\n            :src=\"'./assets/blue-line.png'\"\n            style=\"width: 100%; margin-bottom: 4mm\"\n        />\n      </div>\n\n      <!-- billing details and summary -->\n\n      <div\n          class=\"\"\n          style=\"\n              display: flex;\n              text-transform: uppercase;\n              justify-content: space-between;\n            \"\n      >\n        <div style=\"font-size: 12px\">\n          <h2\n              style=\"\n                  margin-top: 0px;\n                  font-size: 18px;\n                  text-transform: uppercase;\n                  color: #146fc5;\n                  font-weight: 700;\n                  margin-bottom: 3.3mm;\n                  margin-top: 3.3mm;\n                  padding-bottom: 0px;\n                  text-transform: uppercase;\n                \"\n          >\n            Billing & Payment Info\n          </h2>\n\n          <p style=\"padding: 0px; margin: 0px; padding-bottom: 1.5mm\">\n            Paid via <strong>{{ receiptDetails.source }} #{{receiptDetails.transactionCode}}</strong>\n          </p>\n          <p style=\"padding: 0px; margin: 0px; padding-bottom: 1.5mm\">\n            Bill NO. <strong>{{receiptDetails.billNo}}</strong>\n          </p>\n\n          <p\n              style=\"\n                  padding: 0px;\n                  margin: 0px;\n                  padding-top: 4.3mm;\n                  padding-bottom: 1.5mm;\n                \"\n          >\n            Billing Date: <strong>{{ receiptDetails.dateCreated }}</strong>\n          </p>\n          <p style=\"padding: 0px; margin: 0px; padding-bottom: 1.5mm\">\n            Payment date <strong>{{ receiptDetails.dateCreated }}</strong>\n          </p>\n        </div>\n\n        <div>\n          <table\n              style=\"\n                  margin-top: 3.3mm;\n                  border-spacing: inherit;\n                  text-transform: capitalize;\n                  font-size: 14px;\n                \"\n          >\n            <tbody>\n            <tr>\n              <td\n                  style=\"padding: 2.5mm; border-bottom: 1px solid #d9d9d9\"\n              >\n                Amount Billed:\n              </td>\n              <td\n                  style=\"\n                        text-align: right;\n                        padding: 2.5mm;\n                        border-bottom: 1px solid #d9d9d9;\n                      \"\n              >\n                <strong>KES {{receiptDetails.detailAmount}}</strong>\n              </td>\n            </tr>\n\n            <tr>\n              <td\n                  style=\"padding: 2.5mm; border-bottom: 1px solid #d9d9d9\"\n              >\n                Amount Paid:\n              </td>\n              <td\n                  style=\"\n                        text-align: right;\n                        padding: 2.5mm;\n                        border-bottom: 1px solid #d9d9d9;\n                      \"\n              >\n                <strong>KES {{receiptDetails.receiptAmount}}</strong>\n              </td>\n            </tr>\n\n            <tr>\n              <td style=\"padding: 2.5mm\">Balance:</td>\n              <td style=\"text-align: right; padding: 2.5mm\">\n                <strong>KES {{receiptDetails.billBalance}}</strong>\n              </td>\n            </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n\n    <!-- the footer starts here -->\n    <div\n        style=\"\n            padding: 8.5mm;\n            width: 100%;\n            background-color: #e2f2fd;\n            flex-grow: 1;\n            margin-top: 9mm;\n            display: flex;\n            flex-flow: column;\n            align-items: baseline;\n            justify-content: space-between;\n          \"\n    >\n      <div\n          style=\"display: flex; justify-content: space-between; width: 100%\"\n      >\n        <div style=\"font-size: 12px; padding-top: 3.3mm\">\n          <h2\n              style=\"\n                  margin-top: 0px;\n                  font-size: 18px;\n                  text-transform: uppercase;\n                  color: #146fc5;\n                  font-weight: 700;\n                  margin-bottom: 3.3mm;\n                  padding-bottom: 0px;\n                  text-transform: uppercase;\n                \"\n          >\n            County channels platforms\n          </h2>\n\n          <p style=\"padding: 0px; margin: 0px; padding-bottom: 1.5mm\">\n            USSD Code:<strong>{{county.ussd}}</strong>\n          </p>\n          <p style=\"padding: 0px; margin: 0px; padding-bottom: 1.5mm\">\n            Self service portal: <strong>{{county.selfService}}</strong>\n          </p>\n          <p style=\"padding: 0px; margin: 0px; padding-bottom: 1.5mm\">\n            Mobile App: <strong>{{county.mobileApp}}</strong>\n          </p>\n        </div>\n\n        <div style=\"padding-top: 3.3mm; padding-left: 17.3mm\">\n          <div\n              style=\"\n                  display: flex;\n                  margin-bottom: 4mm;\n                  justify-content: center;\n                  align-items: center;\n                \"\n          >\n            <img\n                :src=\"'./assets/android-logo.svg'\"\n                style=\"margin-right: 3.3mm; height: 12mm\"\n            />\n            <span\n                style=\"\n                    font-family: 'Yellowtail', cursive;\n                    color: #146fc5;\n                    line-height: 0.9;\n                    font-size: 20px;\n                  \"\n            >Scan to download the <br/>Android App.</span\n            >\n          </div>\n\n          <div style=\"position: relative; display: flex\">\n            <img\n                :src=\"'./assets/code-demo.png'\"\n                style=\"height: 16.6mm; margin: 1.25mm\"\n            />\n            <img\n                :src=\"'./assets/camera-frame.svg'\"\n                style=\"position: absolute; left: 0; height: 18.6mm\"\n            />\n\n            <img\n                :src=\"'./assets/curved-arrow.svg'\"\n                style=\"height: 13mm; margin-left: 4mm\"\n            />\n          </div>\n        </div>\n      </div>\n\n      <footer style=\"position: relative; width: 100%\">\n        <div style=\"position: absolute; bottom: 0mm\">\n          <div>\n                <span\n                    style=\"\n                    font-family: 'Yellowtail', cursive;\n                    color: #146fc5;\n                    line-height: 0.9;\n                    font-size: 20px;\n                  \"\n                >Thank You!</span\n                >\n            <img :src=\"'./assets/blue-line.png'\" style=\"width: 100%\"/>\n          </div>\n          <div\n              style=\"\n                  display: flex;\n                  align-items: center;\n                  justify-content: space-between;\n                  margin-top: 2mm;\n                \"\n          >\n            <p\n                style=\"\n                    padding-bottom: 10px;\n                    margin-bottom: 0px;\n                    font-size: 12px;\n                    margin-top: 0mm;\n                  \"\n            >\n              Served