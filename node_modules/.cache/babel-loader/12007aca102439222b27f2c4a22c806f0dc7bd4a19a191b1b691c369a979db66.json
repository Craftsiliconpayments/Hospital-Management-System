{"ast":null,"code":"import { execute, trade } from \"@/api\";\nexport default {\n  name: \"TradePermit\",\n  data() {\n    return {\n      permit: {\n        id: null,\n        billNo: null,\n        businessID: null,\n        businessName: null,\n        duration: null,\n        billTotal: null,\n        receiptAmount: null,\n        billBalance: null,\n        startDate: null,\n        endDate: null,\n        statusID: null,\n        status: null,\n        printable: null,\n        issuingOfficer: null,\n        designation: null,\n        dateOfIssue: null,\n        stageStatus: null,\n        comments: null,\n        incomeTypeId: null,\n        feeID: null,\n        subCountyID: null,\n        subCountyName: null,\n        wardID: null,\n        wardName: null,\n        plotNumber: null,\n        physicalAddress: null,\n        buildingName: null,\n        buildingOccupancy: null,\n        floorNo: null,\n        roomNo: null,\n        premiseSize: null,\n        numberOfEmployees: null,\n        tonnage: null,\n        businessDes: null,\n        businessCategory: null,\n        businessSubCategory: null,\n        incomeTypeID: null,\n        businessEmail: null,\n        postalAddress: null,\n        postalCode: null,\n        businessPhone: null,\n        contactPersonNames: null,\n        contactPersonIDNo: null,\n        businessRole: null,\n        contactPersonPhone: null,\n        contactPersonEmail: null,\n        fullNames: null,\n        ownerID: null,\n        ownerPhone: null,\n        ownerEmail: null,\n        kraPin: null,\n        createdBy: null,\n        createdByIDNo: null,\n        dateCreated: null,\n        lat: null,\n        lng: null,\n        lastRenewed: null,\n        permitsCount: null\n      },\n      link: ''\n    };\n  },\n  mounted() {\n    let params = {};\n    try {\n      params = JSON.parse(localStorage['params']);\n      console.log('####' + params.businessID);\n      this.printTradePermit(params.businessID);\n    } catch (error) {\n      // ignore\n    }\n  },\n  methods: {\n    showDate() {\n      const dateObj = new Date();\n      const currentDate = dateObj.getDate() + \"/\" + dateObj.getMonth() + \"/\" + dateObj.getFullYear();\n      return currentDate;\n    },\n    formatPrice(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    printTradePermit(businessID) {\n      const data = new FormData();\n      data.append(\"function\", \"printTradePermit\");\n      data.append(\"businessID\", businessID);\n      execute(data, trade).then(res => {\n        if (res.data.success) {\n          this.permit = res.data.data.permit;\n          this.link = \"https://chart.googleapis.com/chart?chs=300x300&cht=qr&chl=Business ID: \" + this.permit.businessID + \"\\n \" + this.permit.businessName + \" start date: \" + this.permit.startDate + \" Expiry:\" + this.permit.endDate;\n        } else {\n          alert(res.data.data.message);\n        }\n      }).catch(e => {\n        alert(e.message);\n      });\n    },\n    mailTradePermit() {\n      const data = new FormData();\n      data.append(\"function\", \"mailTradePermit\");\n      data.append(\"businessID\", this.$route.params['data']);\n      execute(data, trade).then(res => {\n        alert(res.data.message);\n      }).catch(e => {\n        alert(e.message);\n      });\n    }\n  }\n};","map":{"version":3,"names":["execute","trade","name","data","permit","id","billNo","businessID","businessName","duration","billTotal","receiptAmount","billBalance","startDate","endDate","statusID","status","printable","issuingOfficer","designation","dateOfIssue","stageStatus","comments","incomeTypeId","feeID","subCountyID","subCountyName","wardID","wardName","plotNumber","physicalAddress","buildingName","buildingOccupancy","floorNo","roomNo","premiseSize","numberOfEmployees","tonnage","businessDes","businessCategory","businessSubCategory","incomeTypeID","businessEmail","postalAddress","postalCode","businessPhone","contactPersonNames","contactPersonIDNo","businessRole","contactPersonPhone","contactPersonEmail","fullNames","ownerID","ownerPhone","ownerEmail","kraPin","createdBy","createdByIDNo","dateCreated","lat","lng","lastRenewed","permitsCount","link","mounted","params","JSON","parse","localStorage","console","log","printTradePermit","error","methods","showDate","dateObj","Date","currentDate","getDate","getMonth","getFullYear","formatPrice","value","val","toFixed","replace","toString","FormData","append","then","res","success","alert","message","catch","e","mailTradePermit","$route"],"sources":["/Applications/XAMPP/xamppfiles/htdocs/homabaytest/county-biller/public/PrintDocs/Trade-Permit/TradePermit.vue"],"sourcesContent":["<template>\n\n  <head>\n    <meta charset=\"utf-8\">\n    <title class=\"\">NRK Trade License</title>\n    <link href=\"https://fonts.googleapis.com/css?family=Aldrich|Fira+Sans:200,300,400,500,700,800,900|Norican&display=swap&subset=cyrillic\" rel=\"stylesheet\">\n    <link href=\"https://fonts.googleapis.com/css?family=Libre+Baskerville:400,400i,700|Open+Sans:300,400,600,700&display=swap\" rel=\"stylesheet\">\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\">\n    <link href=\"https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,600;0,700;0,800;1,300;1,400;1,600;1,700;1,800&display=swap\" rel=\"stylesheet\">\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\n    <link href=\"https://fonts.googleapis.com/css2?family=Pridi:wght@200;300;400;500;600;700&display=swap\" rel=\"stylesheet\">\n    <link href=\"https://fonts.googleapis.com/css2?family=Amiri:ital,wght@0,400;0,700;1,400;1,700&family=Fondamento:ital@0;1&display=swap\" rel=\"stylesheet\">\n  </head>\n\n  <body>\n  <page size=\"A4\" class=\"p-2 container w-100 h-100\">\n\n    <h1 style=\"text-transform: uppercase;font-size: 137px;position: absolute;transform: rotate(45deg);top: 63%;opacity: 10%;font-weight: 700;color: green; font-family: 'Pridi', serif; left: 13%;\">00123-23</h1>\n\n\n\n    <section class=\"content-container h-100 w-100 position-relative p-2\" style=\"max-height: 297mm !important\">\n\n      <header style=\"display: flex; height: 47mm; align-items: center; position: relative; width: 100%; justify-content: space-between; padding: 8mm;\">\n        <div>\n          <img src=\"assets/Coat_of_arms_of_Kenya_(Official).svg.png\" style=\"height: 36mm; margin-right: 0mm;\">\n        </div>\n        <div style=\"text-align: center; margin-right: 8.5mm; margin-left: 8.5mm;\">\n          <h2 style=\"\n                        margin-top: 0px;\n                        font-size: 26px;\n                        text-transform: uppercase;\n                        color: #00582f;\n                        font-weight: 700;\n                        margin-bottom: 1.2mm;\n                        padding-bottom: 0px;\n\n                    \">\n            County Government Of Homa-Bay\n          </h2>\n\n          <h4 style=\"padding: 0px;\n                    margin: 0px;\n                    font-size: 18px;\n                    font-weight: bold; text-transform: uppercase; margin-bottom: 4.5mm;\">\n            Homa Bay town Sub County\n          </h4>\n\n          <h4 style=\"padding: 0px;\n                    margin: 0px;\n                    font-size: 14px;\n                    font-weight:500;\">\n            P.O Box 469-40300 Homa Bay <br>\n            Email: finance@homabay.go.ke\n          </h4>\n\n          <h4 style=\"padding: 0px;\n                    margin: 0px;\n                    font-size: 13px;\n                    font-weight: 500; font-style: italic; font-family: 'Times New Roman', Times, serif; margin-top: 2mm;\">\n            <span style=\"font-style: italic; font-family: 'Times New Roman', Times, serif; margin-right: 3mm;\">Tulipe Ushuru Tujenge Homa Bay</span>\n            <span style=\"font-style: italic; font-family: 'Times New Roman', Times, serif;\">#Endless Potential</span>\n          </h4>\n\n        </div>\n        <div>\n          <img :src=\"'./assets/homa-bay.png'\" style=\"height: 36mm; margin-right: 0mm;\">\n        </div>\n\n      </header>\n\n      <div class=\"cert-heading lora-heading my-3\" style=\"padding-top: 0px !important; margin-top: 0.2in !important; text-align: center;\">\n        <h1 class=\"\" style=\"font-family: 'Fondamento', cursive !important; font-size: 60px; margin: 0px; padding: 0px; \"><strong style=\" font-family: 'Fondamento', cursive !important; \">Single Business Permit</strong></h1>\n        <h6 class=\" \" style=\" margin-top: 0px; padding-top: 0px; font-size: 18px; font-weight: 400;font-family:'Pridi', serif !important;; margin-bottom: 0.2in; padding-bottom: 0px; text-transform: uppercase;\">For The Year 2023</h6>\n      </div>\n      <section class=\"container p-3 m-0\">\n        <div class=\"row\">\n          <div class=\"col-12 \">\n            <p class=\"text-center\" style=\"text-align: center;\">\n              <span class=\" \" style=\"text-transform: uppercase; margin-bottom: 3mm;\">\n                <span> County Government of Homa Bay grants this business permit to</span>\n                </span>\n            </P>\n          </div>\n          <div class=\"col-10 pr-0\">\n            <table class=\"table table-bordered mb-0\">\n              <tbody>\n              <!--the name to the busibness-->\n              <tr>\n                <td>\n                  <p class=\"p-0 m-0 green\">Business Name</p>\n                  <p class=\"text-uppercase libre-bold m-0 p-0 title-font\" style=\"font-weight: 500; font-size: 18px;\">We The Business</p>\n                </td>\n                <td>\n                  <p class=\"p-0 m-0 green\">Certificate of incoorporation/ Owner's ID No.</p>\n                  <p class=\"text-uppercase libre-bold m-0 p-0 title-font\" style=\"font-weight: 500; font-size: 18px;\">31834567</p>\n                </td>\n\n              </tr>\n\n              <tr>\n                <td colspan=\"2\">\n                  <span class=\"green\">Owner's Full Name: </span>\n                  <span class=\"text-uppercase libre-bold m-0 p-0 title-font\" style=\"font-weight: 500; font-size: 18px;\">Alex Wanjala Wafula</span>\n                </td>\n\n              </tr>\n\n              <!--the KRA pin-->\n              <tr>\n                <td><span class=\"green\">KRA Pin: </span><span class=\"libre-bold\" style=\"font-weight: 500; font-size: 18px;\">SWR456789</span></td>\n                <td><span class=\"green\">VAT NO.: </span><span class=\"libre-bold\" style=\"font-weight: 500; font-size: 18px;\">SWR456789</span></td>\n\n              </tr>\n\n\n\n              </tbody>\n            </table>\n          </div>\n          <div class=\"col-2 mb-1 pl-0\">\n            <div class=\"locence-no max-height d-flex h-100 justify-content-center align-items-center align-content-center p-2 flex-column\">\n              <p class=\"serial-init green\">Business ID</p>\n              <p> <span class=\"serial-init font-22\"><span class=\"d-none\">BID</span> <span class=\"serial title-font\" style=\"font-family: 'Pridi', serif !important; font-weight: 600;\">HTY67890</span></span>\n              </p>\n            </div>\n          </div>\n          <div class=\"col-10 mt-4 pr-0\">\n            <table class=\"table table-bordered \">\n              <tbody>\n              <tr>\n                <td><span class=\"green\">Business Category: </span><span class=\"libre-bold\" style=\"font-weight: 500; font-size: 18px;\">Informal Sector</span></td>\n              </tr>\n\n              <tr>\n                <td colspan=\"2\">\n                  <p class=\"text-uppercase p-0 m-0 green\">Business Sub Category</p>\n                  <p class=\"libre-bold m-0 p-0 serial m-0\" style=\"font-family: 'Pridi', serif !important;\">Small informal sector trader/service provider e.g. Shoe shiner shoe repairer, street vendor (newspapers, soda, sweets, cigarettes etc.) </p>\n                </td>\n              </tr>\n\n              <tr>\n                <td colspan=\"2\">\n                  <p class=\"text-uppercase p-0 m-0 green\">Activity Description</p>\n                  <p class=\"libre-bold m-0 p-0 serial m-0\" style=\"font-family: 'Pridi', serif !important;\">Shoe shine and repair</p>\n                </td>\n              </tr>\n              </tbody>\n            </table>\n          </div>\n          <div class=\"col-2 mt-4 pl-0\">\n            <div class=\"activity-code-bg max-height d-flex h-100 justify-content-center align-items-center align-content-center p-2 flex-column\">\n              <p class=\"serial-init green\">Activity Code</p>\n              <p>\n                <span class=\"serial-init font-22\">\n                <span class=\"title-font\" style=\"font-family: 'Pridi', serif; font-weight: 400; font-weight: 600;\">415</span>\n               </span>\n              </p>\n            </div>\n          </div>\n\n          <div class=\"col-12 mt-4\">\n            <table class=\"table table-bordered \">\n              <tbody>\n              <tr>\n                <td class=\"text-uppercase green\">Amount Paid</td>\n                <td class=\"title-font\" style=\"letter-spacing: 2px; font-family: 'Pridi', serif; font-weight: 600; font-size: 18px;\"> KES 25,4589</td>\n              </tr>\n              <tr>\n                <td class=\"other-num serial-init\" colspan=\"2\"><span class=\"green\">Amount in words </span> <span  style=\"font-weight: 500; font-size: 18px; font-family: 'Pridi', serif;\"> *** Two thousand two hundred ***</span></td>\n              </tr>\n\n              </tbody>\n            </table>\n          </div>\n\n\n          <div class=\"col-12 mt-4\">\n            <table class=\"table table-bordered\">\n              <tbody>\n\n              <tr class=\"d-none\">\n                <td colspan=\"3\" class=\"green\">Business under this permit shall be conducted at the address as indicated below</td>\n              </tr>\n              <tr>\n                <td colspan=\"2\"><span class=\"green\">P.O. Box : </span><span class=\"libre-bold\">541 Nairobi</span></td>\n                <td>Plot No. : <span class=\"libre-bold title-font\">12202/1250</span></td>\n              </tr>\n              <tr>\n                <td colspan=\"3\"><span class=\"green\">Road Street : </span><span class=\"title-font\" style=\"font-weight:400;font-family:'Pridi', serif;\">Along the bungma road</span></td>\n              </tr>\n              <tr>\n                <td><span class=\"green\">Building : </span><span class=\"libre-bold\">The Tower</span></td>\n                <td><span class=\"green\">Floor : </span><span class=\"libre-bold\">124</span></td>\n                <td><span class=\"green\">Door/Stall No. : </span><span class=\"libre-bold\">123</span></td>\n              </tr>\n              </tbody>\n            </table>\n          </div>\n          <div class=\"col-12 mt-4\">\n            <div class=\"row m-0\">\n              <div class=\"col-6 p-0\">\n                <div>\n                  <div class=\"p-2 issue-date\">\n                    <span class=\"d-none\">License Covers For:</span>\n                    <hr class=\"py-0 my-0 d-none\">\n                    <p class=\"d-none\">Fire License</p>\n                    <hr class=\"py-0 my-0 d-none\">\n                    <p class=\"d-none\">Food Hygiene License</p>\n                    <hr class=\"py-0 my-0 d-none\">\n                    <p class=\"d-none\">Advertisement License</p>\n                  </div>\n                </div>\n\n                <div>\n                  <div class=\"p-2 issue-date\" style=\"border-top: 0px !important;\">\n                    <span class=\"green\">Licensing officer: </span><span class=\"\" style=\"font-weight:400;font-family:'Pridi', serif;\">Alex Wanjala</span>\n                    <br>\n                    <span class=\"green\">Date Of Issue: </span><span class=\"\" style=\"font-weight:400;font-family:'Pridi', serif;\">10 Jan 2023</span>\n\n                  </div>\n                </div>\n\n\n                <div>\n                  <div class=\"p-2 issue-date\" style=\"border-top: 0px !important;\">\n\n                    <span class=\"green\">Expiry Date: </span><br><span style=\"font-weight:400;font-family:'Pridi', serif;\">31 Dec 2023</span>\n                  </div>\n                </div>\n\n                <div class=\"w-100 the-seal-container\">\n                  <img src=\"assets/seal.png\" class=\"the-seal\">\n                </div>\n              </div>\n\n              <div class=\"col-6 p-0\">\n                <div class=\"new-qr-container p-2\">\n\n\n                  <p class=\"green\">By order of</p>\n\n                  <div class=\"stamp-qr\">\n\n                    <img src=\"assets/sign.png\" alt=\" The signature \" class=\"the-stamp \">\n                    <img class=\"mb-2 float-left theqr \" src=\"assets/code-demo.png \">\n                  </div>\n                </div>\n              </div>\n\n              <div>\n                <div style=\"display: flex; justify-content: space-between; align-items:center; font-size: 12px; font-weight: 500;\">\n                  <span>Permit No.: <span style=\"font-weight: bold; color: red;\">00123-23</span></span>\n                  <span>Date Downloaded 23 Oct 2023 12:34 AM</span>\n                </div>\n                <img src=\"assets/green-line.png\" style=\"width: 100%;\">\n              </div>\n\n              <div class=\"col-12\" style=\"font-size: 12px; padding: 0px; margin: 0px;\">\n                <h4 style=\"text-transform: uppercase;\">County Government of Homa Bay</h4>\n                <p class=\"red font-14 \" style=\"font-weight: 500; font-size: 12px !important;\">\n                  Notice: Granting this permit <strong>DOES NOT EXEMPT</strong> the business identified above from complying with current regulations on Health and safety as established by the Government of Kenya and the\n                  <strong style=\"text-transform: uppercase;\">County Government of Homa Bay</strong>\n                </p>\n              </div>\n            </div>\n          </div>\n\n\n        </div>\n      </section>\n\n      <img  src=\"assets/nbk_logo.svg\" style=\"display: none; bottom: auto;\" Logo class=\"nbk\">\n\n\n    </section>\n\n  </page>\n  <button class=\"print-btn \" onclick=\"window.print() \"><img src=\"assets/printer.svg \" alt=\"Printer Icon \"></button>\n  </body>\n\n</template>\n\n<script>\nimport {execute, trade} from \"@/api\";\n\n\nexport default {\n  name: \"TradePermit\",\n  data() {\n    return {\n      permit: {\n        id: null,\n        billNo: null,\n        businessID: null,\n        businessName: null ,\n        duration: null,\n        billTotal: null,\n        receiptAmount: null,\n        billBalance: null,\n        startDate: null,\n        endDate: null,\n        statusID: null,\n        status: null,\n        printable: null,\n        issuingOfficer: null,\n        designation: null,\n        dateOfIssue: null,\n        stageStatus: null,\n        comments: null,\n        incomeTypeId: null,\n        feeID: null,\n        subCountyID: null,\n        subCountyName: null,\n        wardID: null,\n        wardName: null,\n        plotNumber: null,\n        physicalAddress: null,\n        buildingName: null,\n        buildingOccupancy: null,\n        floorNo: null,\n        roomNo: null,\n        premiseSize: null,\n        numberOfEmployees: null,\n        tonnage: null,\n        businessDes: null,\n        businessCategory: null,\n        businessSubCategory: null,\n        incomeTypeID: null,\n        businessEmail:null,\n        postalAddress: null,\n        postalCode: null,\n        businessPhone: null,\n        contactPersonNames: null,\n        contactPersonIDNo: null,\n        businessRole: null,\n        contactPersonPhone: null,\n        contactPersonEmail: null,\n        fullNames: null,\n        ownerID: null,\n        ownerPhone: null,\n        ownerEmail: null,\n        kraPin: null,\n        createdBy: null,\n        createdByIDNo: null,\n        dateCreated: null,\n        lat: null,\n        lng: null,\n        lastRenewed: null,\n        permitsCount: null\n      },\n      link:''\n    };\n  },\n  mounted() {\n    let params = {}\n    try {\n\n      params = JSON.parse(localStorage['params'])\n      console.log('####'+ params.businessID)\n      this.printTradePermit(params.businessID);\n    } catch (error) {\n      // ignore\n    }\n\n  },\n  methods: {\n    showDate() {\n      const dateObj = new Date();\n      const currentDate =\n          dateObj.getDate() +\n          \"/\" +\n          dateObj.getMonth() +\n          \"/\" +\n          dateObj.getFullYear();\n      return currentDate;\n    },\n    formatPrice(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    printTradePermit(businessID) {\n      const data = new FormData();\n      data.append(\"function\", \"printTradePermit\");\n      data.append(\"businessID\", businessID);\n\n      execute(data,trade)\n          .then((res) => {\n            if (res.data.success) {\n              this.permit = res.data.data.permit;\n              this.link = \"https://chart.googleapis.com/chart?chs=300x300&cht=qr&chl=Business ID: \"+this.permit.businessID+\"\\n \"\n                  +this.permit.businessName+\" start date: \"+ this.permit.startDate+\" Expiry:\"+this.permit.endDate;\n            } else {\n              alert(res.data.data.message);\n            }\n          })\n          .catch((e) => {\n            alert(e.message);\n       