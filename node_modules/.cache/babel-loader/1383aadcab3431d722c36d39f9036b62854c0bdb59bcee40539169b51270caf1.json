{"ast":null,"code":"/* eslint-disable */\nimport NavigationBar from \"@/components/Navigation\";\nimport { biller, execute } from \"@/api\";\nimport router from \"@/router\";\nimport moment from \"moment\";\nexport default {\n  name: \"ReportsStreams\",\n  components: {\n    NavigationBar\n  },\n  data() {\n    return {\n      dateTo: '',\n      dateFrom: '',\n      message: '',\n      alert: {\n        success: false,\n        info: false,\n        failed: false\n      },\n      contact: {\n        email: ''\n      },\n      password: '',\n      streams: [{\n        stream: \"ADVERTS\",\n        transactions: \"33\",\n        amount: \"801905\",\n        agents: \"5\"\n      }]\n    };\n  },\n  mounted() {\n    this.dateFrom = moment().startOf('month').format('YYYY-MM-DD');\n    this.dateTo = moment().format('YYYY-MM-DD');\n    this.$watch('dateFrom', function (newValue) {\n      if (this.dateFrom === '' || this.dateTo === '') {} else {\n        this.getReportsStreams();\n      }\n    });\n    this.$watch('dateTo', function (newValue) {\n      if (this.dateFrom === '' || this.dateTo === '') {} else {\n        this.getReportsStreams();\n      }\n    });\n    this.getReportsStreams();\n  },\n  methods: {\n    getReportsStreams() {\n      this.streams.splice(0);\n      const data = new FormData();\n      data.append(\"function\", \"getReportsStreams\");\n      data.append(\"dateFrom\", this.dateFrom);\n      data.append(\"dateTo\", this.dateTo);\n      execute(data, biller).then(res => {\n        this.alert.info = false;\n        if (res.data.success) {\n          this.streams = res.data.data.streams;\n        } else {\n          this.message = res.data.message;\n        }\n      }).catch(e => {\n        //alert(e.message);\n        this.message = e.message;\n      });\n    }\n  }\n};","map":{"version":3,"names":["NavigationBar","biller","execute","router","moment","name","components","data","dateTo","dateFrom","message","alert","success","info","failed","contact","em